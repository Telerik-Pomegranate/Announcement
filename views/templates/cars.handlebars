 <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <img class="slide-image" src="../../assets/img/Fleet_images_02.png" alt="" style="width: 1150px; height: 300px" >
                <h1 class="page-header">Cars
                </h1>
            </div>
        </div>
          {{#items}}
        <div class="row">
            <div class="col-md-3">
                <a href="#/cars/announcement/{{id}}" id="{{id}}" name="asd">
                    <img class="img-responsive" src="{{url}}" alt="" style="margin:auto;height:auto;x border:1px solid;border-radius:10%;max-width: 270px;max-height: 181px;">
                </a>
            </div>
            <div class="col-md-9" style="color:black;margin:-2% 0 -2% 0">
                <h3>{{head}}</h3>
                <h4>{{subheading}}</h4>
                <strong style="color:indigo;  font-size: 100%;" >Price: </strong>
                <h3 style="display:inline-block; margin-top:0%">{{price}}</h3>
                <p style="height:20px; width:300px; text-overflow: ellipsis;white-space: nowrap;overflow: hidden;">{{body}}</p>
                <a class="btn btn-primary" href="#/cars/announcement/{{id}}" style="display:block">More details... <span class="glyphicon glyphicon-chevron-right"></span></a>
            <h3>Mobile number: {{gsm}}</h3>
           </div>
        </div>
        <hr>
   {{/items}}
        <div class="row text-center">
            <div class="col-lg-12">
                <ul class="pagination">
                    <li class="prev" style="cursor: pointer">
                        <a>&laquo;</a>
                    </li>
                    <li>
                        <a href="#/cars/1">1</a>
                    </li>
                    <li class="next" style="cursor: pointer">
                        <a>&raquo;</a>
                    </li>
                </ul>
            </div>
        </div>
        <hr>
    </div>