   <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <h1 class="page-header">{{items.head}} for: {{items.price}}
                    <small style="float:right">contact: {{id.gsm}}, email: {{id.email}}</small>
                </h1>
            </div>
        </div>
        <div class="row">

            <div class="col-md-4">
                <img class="img-responsive" src="{{items.url}}" alt="" style=" border:1px solid;border-radius:10%; max-width: 350px;max-height: 300px;" >
            </div>

            <div class="col-md-7" style="color:black;">
                <h3>{{items.subheading}}</h3>
                <p>{{items.body}}</p>


                 <h4>Published By: <a href="#/announ-on-user/1/{{items.currentUserId}}" style="color:darkslategray">{{items.userName}}</a></h4>

            </div>
            <div class="row">

            <div class="col-lg-12">
                {{!--<h3 class="page-header">Related images</h3>     --}}
                <hr style="width:97%">
            </div>
<div style="margin-left: 16px;">
    {{#items.otherUrl}}
            <div class="col-sm-2 col-xs-6">
                <a class="views-other-image" style=" cursor: pointer;">
                    <img class="img-responsive portfolio-item " src="{{this}}" alt="" style="max-width:100px;max-height:200px">
                </a>
            </div>
{{/items.otherUrl}}
            
</div>
        </div>
        </div>
        
        <div style="float:right; margin-top: -10px;">
 <iframe src="https://www.facebook.com/plugins/share_button.php?href=https%3A%2F%2Fdevelopers.facebook.com%2Fdocs%2Fplugins%2F&layout=button_count&size=small&mobile_iframe=true&width=86&height=20&appId" width="86" height="20" style="border:none;overflow:hidden" scrolling="no" frameborder="0" allowTransparency="true"></iframe>
        </div>
